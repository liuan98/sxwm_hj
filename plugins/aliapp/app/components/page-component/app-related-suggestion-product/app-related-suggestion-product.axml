<block a:if="{{goods_list.length>0}}"><view class="app-related-suggestion-product data-v-65227070"><block a:if="{{isRecommend}}"><view class="app-top dir-left-nowrap main-center cross-center data-v-65227070"><view class="app-content dir-left-nowrap main-between cross-center data-v-65227070"><view class="app-line data-v-65227070"></view><view class="app-icon-love image-cover image-no-rep data-v-65227070"></view><text class="app-text data-v-65227070">{{text}}</text><view class="app-line data-v-65227070"></view></view></view></block><view class="app-bottom dir-top-nowrap data-v-65227070"><block a:for="{{goods_list}}" a:for-item="item" a:for-index="index" a:key="index"><view class="row dir-left-nowrap main-between box-grow-1 data-v-65227070"><block a:for="{{item}}" a:for-item="good" a:for-index="key" a:key="key"><view data-event-opts="{{[['tap',[['route_jump',['$0'],[[['goods_list','',index],['','',key]]]]]]]}}" class="item box-grow-0 data-v-65227070" onTap="__e"><view class="image-name dir-top-nowrap main-left data-v-65227070"><image class="app-image data-v-65227070" lazy-load="true" src="{{good.cover_pic}}"></image><block a:if="{{good.goods_stock==0&&appSetting.is_show_stock=='1'}}"><view class="out-dialog data-v-65227070"><image src="{{appSetting.is_use_stock=='1'?appImg.book_out:appSetting.sell_out_pic}}" class="data-v-65227070"></image>\</view></block><block a:if="{{isShowGoodsName==1}}"><view class="name data-v-65227070">{{good.name}}</view></block></view><block a:if="{{sign==='advance'}}"><view class="advance-content data-v-65227070"><block a:if="{{good.advanceGoods}}"><view class="deposit data-v-65227070">{{'定金￥'+good.advanceGoods.deposit+"抵"+good.advanceGoods.swell_deposit+''}}</view></block></view></block><view class="{{(('content dir-top-nowrap main-right data-v-65227070')+' '+(''+(sign!=='advance'?'content-no':'content-yes')))}}"><block a:if="{{good.is_level==1}}"><view class="member-price data-v-65227070"><app-member-price vue-id="{{'1-'+index+'-'+key}}" price="{{good.level_price}}" class="data-v-65227070" onVueInit="__l"></app-member-price></view></block><block a:if="{{good.vip_card_appoint.discount>0}}"><view class="sup-vip data-v-65227070"><app-sup-vip vue-id="{{'2-'+index+'-'+key}}" is_vip_card_user="{{good.vip_card_appoint.is_vip_card_user}}" discount="{{good.vip_card_appoint.discount}}" class="data-v-65227070" onVueInit="__l"></app-sup-vip></view></block><view class="price-total dir-left-nowrap main-between data-v-65227070"><text class="price data-v-65227070">{{''+(sign==='advance'?'￥':'')+good.price_content+''}}</text><block a:if="{{isShowSalesNum==1}}"><text class="volume data-v-65227070">{{''+good.sales+''}}</text></block></view></view></view></block></view></block></view><block a:if="{{!request}}"><app-load-text vue-id="3" class="data-v-65227070" onVueInit="__l"></app-load-text></block></view></block>