<app-layout vue-id="1" class="data-v-29b19ddd" onVueInit="__l"><block a:if="{{detail.status==1}}"><app-index vue-id="{{('2')+','+('1')}}" setting="{{setting}}" detail="{{detail}}" class="data-v-29b19ddd" onVueInit="__l"></app-index></block><block a:else><view class="other data-v-29b19ddd"><view class="header data-v-29b19ddd"><block a:if="{{status}}"><image src="{{setting.form.bg_url=='statics/img/app/stock/banner.png'?stock.banner:setting.form.bg_url}}" class="data-v-29b19ddd"></image></block></view><block a:if="{{look}}"><app-condition vue-id="{{('3')+','+('1')}}" read="{{read}}" setting="{{setting}}" rate="{{rate}}" check="{{check}}" data-event-opts="{{[['^toRead',[['toRead']]],['^beRead',[['beRead']]],['^submit',[['submit']]],['^update',[['receive']]]]}}" onToRead="__e" onBeRead="__e" onSubmit="__e" onUpdate="__e" class="data-v-29b19ddd" onVueInit="__l"></app-condition></block><block a:if="{{wait}}"><view class="wait data-v-29b19ddd"><image src="{{bonusImg.wait}}" class="data-v-29b19ddd"></image><view class="data-v-29b19ddd">谢谢您的支持，请等待审核...</view><view data-event-opts="{{[['tap',[['toIndex',['$event']]]]]}}" class="apply-sumbit to-mall data-v-29b19ddd" onTap="__e"><button class="data-v-29b19ddd">去商城逛逛</button></view></view></block><block a:if="{{beApply&&!wait}}"><view class="apply-area data-v-29b19ddd"><view class="apply data-v-29b19ddd"><view class="apply-input data-v-29b19ddd" style="border-top:0;">欢迎申请股东分红，请填写申请信息</view><view class="apply-input dir-left-nowrap data-v-29b19ddd"><view class="label data-v-29b19ddd">姓名</view><input placeholder="请填写真实姓名" placeholder-style="color:#cdcdcd" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" onInput="__e" class="data-v-29b19ddd"/></view><view class="apply-input dir-left-nowrap data-v-29b19ddd"><view class="label data-v-29b19ddd">手机号</view><input type="number" placeholder="请填写手机号" placeholder-style="color:#cdcdcd" data-event-opts="{{[['input',[['__set_model',['','mobile','$event',[]]]]]]}}" value="{{mobile}}" onInput="__e" class="data-v-29b19ddd"/></view><block a:if="{{setting.is_agreement==1}}"><view class="apply-input cross-center data-v-29b19ddd"><view data-event-opts="{{[['tap',[['beRead',['$event']]]]]}}" class="read data-v-29b19ddd" onTap="__e"><block a:if="{{read==false}}"><image src="/static/image/icon/icon-uncheck.png" class="data-v-29b19ddd"></image></block><block a:else><image src="/static/image/icon/icon-checkbox-checked-a.png" class="data-v-29b19ddd"></image></block></view><text class="data-v-29b19ddd">已阅读并同意《</text><text data-event-opts="{{[['tap',[['toRead',['$event']]]]]}}" class="agreement_title data-v-29b19ddd" onTap="__e">{{setting.agreement_title?setting.agreement_title:'股东分红申请协议'}}</text><text class="data-v-29b19ddd">》</text></view></block></view><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="apply-sumbit be-apply data-v-29b19ddd" onTap="__e"><button class="data-v-29b19ddd">申请成为股东</button></view></view></block><block a:if="{{refuse||remove}}"><view class="refuse data-v-29b19ddd"><image src="./../image/refuse.png" class="data-v-29b19ddd"></image><block a:if="{{refuse}}"><view class="data-v-29b19ddd">您的申请被拒</view></block><block a:if="{{remove}}"><view class="data-v-29b19ddd">您被解除股东身份</view></block><view class="refuse-info data-v-29b19ddd"><view class="data-v-29b19ddd">审核时间:<text class="data-v-29b19ddd">{{detail.updated_at}}</text></view><block a:if="{{refuse}}"><view class="refuse-reason data-v-29b19ddd">拒绝理由:<text style="word-wrap:break-word;" class="data-v-29b19ddd">{{detail.reason?detail.reason:"无"}}</text></view></block><block a:if="{{remove}}"><view class="refuse-reason data-v-29b19ddd">解除理由:<text style="word-wrap:break-word;" class="data-v-29b19ddd">{{detail.reason?detail.reason:"无"}}</text></view></block><view data-event-opts="{{[['tap',[['reApply',['$event']]]]]}}" class="apply-sumbit to-mall data-v-29b19ddd" onTap="__e"><button class="data-v-29b19ddd">再次申请</button></view></view></view></block><block a:if="{{beApply&&!wait}}"><image class="rights data-v-29b19ddd" style="{{'height:'+(height+'px')+';'}}" src="{{setting.form.bottom_bg_url=='statics/img/app/stock/foot.png'?stock.foot:setting.form.bottom_bg_url}}" data-event-opts="{{[['load',[['imageLoad',['$event']]]]]}}" onLoad="__e"></image></block></view></block><block a:if="{{protocol}}"><view class="modal cross-center main-center data-v-29b19ddd"><view class="protocol data-v-29b19ddd"><view class="protocol-name t-omit data-v-29b19ddd">{{setting.agreement_title}}</view><view class="protocol-content data-v-29b19ddd"><app-rich-text vue-id="{{('4')+','+('1')}}" content="{{protocol}}" class="data-v-29b19ddd" onVueInit="__l"></app-rich-text></view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" onTap="__e" class="data-v-29b19ddd"><view class="read-over data-v-29b19ddd">我已阅读</view></view></view></view></block></app-layout>