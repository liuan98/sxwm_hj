<view class="app-product-list data-v-2c884612"><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['route_go',['$0'],[[['list','',index]]]]]]]}}" class="app-item dir-left-nowrap data-v-2c884612" onTap="__e"><image class="app-image data-v-2c884612" src="{{item.goods.cover_pic}}"></image><block a:if="{{item.miaosha_num==0&&appSetting.is_show_stock=='1'}}"><view class="out-dialog data-v-2c884612"><image src="{{appSetting.is_use_stock=='1'?appImg.plugins_out:appSetting.sell_out_pic}}" class="data-v-2c884612"></image></view></block><view class="app-content data-v-2c884612"><text class="app-name data-v-2c884612">{{item.goods.name}}</text><view class="app-percentage data-v-2c884612"><view style="{{'width:'+(''+item.goods.miaosha_percentage)+';'}}" class="data-v-2c884612"></view></view><view class="app-text dir-left-nowrap main-between cross-center data-v-2c884612"><text class="app-already data-v-2c884612">{{"已抢购"+item.goods.miaosha_count+"件"}}</text><text class="app-past data-v-2c884612">{{"仅剩"+item.goods.miaosha_num+"件"}}</text></view><view class="price-button dir-left-nowrap main-between cross-bottom data-v-2c884612"><view class="price app-price dir-top-nowrap main-right cross-top data-v-2c884612"><block a:if="{{item.is_level}}"><view class="data-v-2c884612"><app-member-price vue-id="{{'1-'+index}}" price="{{item.level_price}}" class="data-v-2c884612" onVueInit="__l"></app-member-price></view></block><block a:if="{{item.vip_card_appoint.discount>0}}"><app-sup-vip vue-id="{{'2-'+index}}" is_vip_card_user="{{item.vip_card_appoint.is_vip_card_user}}" margin="4rpx 0 0" discount="{{item.vip_card_appoint.discount}}" class="data-v-2c884612" onVueInit="__l"></app-sup-vip></block><view class="app-min-price data-v-2c884612">{{"FCFA"+item.goods.min_price}}</view><view class="app-original-price data-v-2c884612">{{"FCFA"+item.goods.original_price}}</view></view><block a:if="{{status===1&&item.miaosha_num>0}}"><view class="app-button data-v-2c884612">马上抢购</view></block><block a:if="{{(status===0||status===2)&&item.miaosha_num>0}}"><view class="app-button no-button data-v-2c884612">查看商品</view></block><block a:if="{{item.miaosha_num==0}}"><view class="app-button data-v-2c884612" style="background-color:#CDCDCD;color:#fff;">下次再来</view></block></view></view></view></block><block a:if="{{empty}}"><view style="{{'width:'+('100%')+';'+('height:'+(botHeight+'rpx')+';')}}" class="data-v-2c884612"></view></block></view>
