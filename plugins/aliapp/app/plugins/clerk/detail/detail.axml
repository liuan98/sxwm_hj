<app-layout vue-id="1" class="data-v-30674ff3" onVueInit="__l"><view class="status data-v-30674ff3"><image class="status-bg data-v-30674ff3" src="{{clerkImg.detail}}"></image><block a:if="{{detail.clerk_id>0&&detail.cancel_status==0}}"><view class="status-text data-v-30674ff3">已核销</view></block><block a:if="{{detail.clerk_id>0}}"><view class="status-info data-v-30674ff3"><block a:if="{{card_id>0}}"><view class="data-v-30674ff3">{{"核销时间："+detail.clerked_at}}</view></block><block a:if="{{id>0}}"><view class="data-v-30674ff3">{{"核销时间："+detail.orderClerk.updated_at}}</view></block><block a:if="{{id>0}}"><view class="status-price data-v-30674ff3">{{"核销金额：FCFA"+detail.total_pay_price}}</view></block></view></block><block a:if="{{detail.clerk_id==0&&(card_id>0||detail.cancel_status==0)}}"><view class="status-text data-v-30674ff3">未核销</view></block><block a:if="{{detail.cancel_status==1}}"><view class="status-text data-v-30674ff3">已退款</view></block><block a:if="{{detail.cancel_status==1}}"><view class="status-info data-v-30674ff3"><block a:if="{{id>0}}"><view class="data-v-30674ff3">{{"退款时间："+detail.cancel_time}}</view></block></view></block><block a:if="{{id>0}}"><view class="address-info data-v-30674ff3"><image src="./../image/address.png" class="data-v-30674ff3"></image><view class="data-v-30674ff3">{{(detail.store.name?detail.store.name:'')+" "+(detail.store.mobile?detail.store.mobile:'')}}</view><view class="address data-v-30674ff3">{{detail.store.address}}</view></view></block><block a:if="{{card_id>0}}"><view class="card-info data-v-30674ff3"><view class="dir-left-nowrap cross-center data-v-30674ff3"><image class="card-img data-v-30674ff3" src="{{detail.pic_url}}"></image><view class="card-name t-omit-two data-v-30674ff3">{{detail.name}}</view></view><view class="card-other data-v-30674ff3"><view class="other-label data-v-30674ff3">使用时间</view><view class="data-v-30674ff3">{{detail.start_time+" - "+detail.end_time}}</view><view class="other-label data-v-30674ff3">发放时间