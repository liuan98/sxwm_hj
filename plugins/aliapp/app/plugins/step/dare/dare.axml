<app-layout vue-id="1" class="data-v-a9f32fe0" onVueInit="__l"><block a:if="{{join||success}}"><view class="join-bg data-v-a9f32fe0"><block a:if="{{success}}"><view class="success data-v-a9f32fe0"><view class="content data-v-a9f32fe0"><image class="banner data-v-a9f32fe0" src="./../image/submit.png"></image><view class="banner-text data-v-a9f32fe0">恭喜您</view><image class="success-tip data-v-a9f32fe0" src="./../image/success.png"></image><view class="data-v-a9f32fe0">成功提交步数</view><view class="data-v-a9f32fe0">奖金将于24点结算后自动发放</view><view class="tip data-v-a9f32fe0">{{"奖金可至“我的"+(setting.currency_name?setting.currency_name:'活力币')+"”查看"}}</view><view data-event-opts="{{[['tap',[['closeTip',[1]]]]]}}" onTap="__e" class="data-v-a9f32fe0"><button class="success-btn data-v-a9f32fe0">我知道了</button></view></view></view></block><block a:if="{{join}}"><view class="join-success data-v-a9f32fe0"><image class="join-img data-v-a9f32fe0" src="./../image/participation.png"></image><block a:if="{{join}}"><view data-event-opts="{{[['tap',[['closeTip']]]]}}" class="join-close data-v-a9f32fe0" onTap="__e"><image src="/static/image/icon/icon-close.png" class="data-v-a9f32fe0"></image></view></block><view class="data-v-a9f32fe0">{{"报名成功，记得"+open_date+"来参加哦"}}</view><button class="invite data-v-a9f32fe0" open-type="share">邀请好友</button></view></block></view></block><view class="bg data-v-a9f32fe0"><image src="{{setting.activity_pic.length>0?setting.activity_pic:stepImg.app_image.activity_bg}}" class="data-v-a9f32fe0"></image></view><view class="icon data-v-a9f32fe0"><view data-event-opts="{{[['tap',[['toLog',['$event']]]]]}}" style="display:inline-block;" onTap="__e" class="data-v-a9f32fe0"><image src="./../image/log.png" class="data-v-a9f32fe0"></image></view><view data-event-opts="{{[['tap',[['toRules',['$event']]]]]}}" style="display:inline-block;" onTap="__e" class="data-v-a9f32fe0"><image src="./../image/rule.png" class="data-v-a9f32fe0"></image></view></view><block a:if="{{list[0].id>0}}"><view class="dare data-v-a9f32fe0"><block a:for="{{list}}" a:for-item="item" a:for-index="__i0__" a:key="id"><view class="join data-v-a9f32fe0"><view class="join-title data-v-a9f32fe0"><view class="title-left data-v-a9f32fe0">{{item.begin_at+"期"}}</view><block a:if="{{item.date!==time}}"><view class="msg data-v-a9f32fe0">{{"已有"+item.people_num+"人报名"}}</view></block><block a:if="{{item.log_status==null}}"><view class="not data-v-a9f32fe0">未参赛</view></block><block a:if="{{item.log_status!=nul}}"><view class="already data-v-a9f32fe0">已参赛</view></block></view><view class="activity-name data-v-a9f32fe0">{{item.title}}</view><block a:if="{{item.log_status==null&&item.now_time_status==true}}"><view class="activity-step data-v-a9f32fe0">{{"目标"+item.step_num+"步"}}</view></block><block a:if="{{item.log_status!=null&&item.now_time_status==true}}"><view class="join-info data-v-a9f32fe0"><view class="target data-v-a9f32fe0">目标<text class="data-v-a9f32fe0">{{item.step_num}}</text>步，已完成<block a:if="{{item.log_status==='0'}}"><text class="data-v-a9f32fe0">{{daily_real_num}}</text></block><block a:if="{{item.log_status>0}}"><text class="data-v-a9f32fe0">{{item.user_total_num}}</text></block>步</view><block a:if="{{item.log_status!=1}}"><view class="data-v-a9f32fe0">请于今日24点前，更新并提交步数</view></block><block a:if="{{item.log_status==1}}"><view class="data-v-a9f32fe0">奖金将于24点后结算自动发放</view></block><block a:if="{{item.log_status==0&&daily_real_num>item.step_num-1}}"><view data-event-opts="{{[['tap',[['toSubmit',['$0','$1'],[[['list','id',item.id,'step_num']],[['list','id',item.id,'id']]]]]]]}}" onTap="__e" class="data-v-a9f32fe0"><button class="submit data-v-a9f32fe0">提交成绩</button></view></block><block a:if="{{item.log_status==1}}"><button class="submit-end data-v-a9f32fe0">已提交成绩</button></block></view></block><block a:if="{{item.now_time_status==true}}"><view class="dare-item data-v-a9f32fe0"><view class="item data-v-a9f32fe0"><view class="total data-v-a9f32fe0">{{item.currency_num?item.currency_num:0}}</view><view class="data-v-a9f32fe0">奖金池总额</view></view><view class="item data-v-a9f32fe0"><view class="total data-v-a9f32fe0">{{item.award_num?item.award_num:0}}</view><view class="data-v-a9f32fe0">达标人数</view></view></view></block><block a:if="{{item.now_time_status==false}}"><view class="data-v-a9f32fe0"><view class="money data-v-a9f32fe0">{{item.currency_num?item.currency_num:0}}</view><view class="data-v-a9f32fe0">奖金池总额</view><view class="info data-v-a9f32fe0">参与满<text class="data-v-a9f32fe0">{{item.step_num}}</text>步起平分奖池金额</view><block a:if="{{item.log_status==null}}"><view data-event-opts="{{[['tap',[['toJoin',['$0'],[[['list','id',item.id,'id']]]]]]]}}" class="join-btn data-v-a9f32fe0" onTap="__e"><button class="join-btn data-v-a9f32fe0">参与比赛</button></view></block><block a:if="{{item.log_status==0}}"><button class="finish-btn data-v-a9f32fe0" open-type="share">已参赛，邀请好友</button></block></view></block></view></block><block a:if="{{ad_data}}"><view class="temp data-v-a9f32fe0"></view></block><block a:if="{{loading}}"><view class="loading data-v-a9f32fe0">正在加载中...</view></block><block a:if="{{ad_data}}"><view class="temp data-v-a9f32fe0"><block a:if="{{ad_data.unit_id}}"><ad unit-id="{{ad_data.unit_id}}" data-event-opts="{{[['error',[['adError',['$event']]]]]}}" onError="__e" class="data-v-a9f32fe0"></ad></block></view></block></view></block><block a:else><view class="space data-v-a9f32fe0"><image src="./../image/none.png" class="data-v-a9f32fe0"></image><view class="data-v-a9f32fe0">暂无挑战赛哦</view><block a:if="{{ad_data}}"><view class="temp data-v-a9f32fe0"><block a:if="{{ad_data.unit_id}}"><ad unit-id="{{ad_data.unit_id}}" data-event-opts="{{[['error',[['adError',['$event']]]]]}}" onError="__e" class="data-v-a9f32fe0"></ad></block></view></block></view></block><view class="ad data-v-a9f32fe0"><block a:if="{{ad_data.unit_id}}"><ad unit-id="{{ad_data.unit_id}}" data-event-opts="{{[['error',[['adError',['$event']]]]]}}" onError="__e" class="data-v-a9f32fe0"></ad></block></view><block a:if="{{isSubmit}}"><view class="dialog-bg cross-center data-v-a9f32fe0"><view class="dialog data-v-a9f32fe0"><view class="dialog-title data-v-a9f32fe0">提示</view><view class="main-center refund-tip data-v-a9f32fe0">确认提交成绩？</view><view class="main-center btn-area data-v-a9f32fe0"><view data-event-opts="{{[['tap',[['cancel',['$event']]]]]}}" class="submit-btn data-v-a9f32fe0" onTap="__e">取消</view><view class="line data-v-a9f32fe0"></view><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="submit-btn be-submit data-v-a9f32fe0" onTap="__e">确认</view></view></view></view></block></app-layout>