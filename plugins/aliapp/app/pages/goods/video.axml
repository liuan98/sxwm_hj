<app-layout vue-id="1" onVueInit="__l"><view class="page" style="{{'height:'+(height)+';'}}"><view class="swiper" style="{{(containerStyle)}}"><block a:for="{{videoList}}" a:for-item="item" a:for-index="index" a:key="index"><block><video-new class="video" style="{{'height:'+(height)+';'+('width:'+(width)+';')}}" vue-id="{{('2-'+index)+','+('1')}}" video_id="{{item.video_id}}" video_url="{{item.video_url}}" height="{{height}}" width="{{width}}" play="{{item.flag}}" cur_index="{{cur_index}}" onVueInit="__l"></video-new></block></block></view><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="close" catchTap="__e"><image class="image" src="/static/image/icon/video-close.png"></image></view><view data-event-opts="{{[['tap',[['clickVideo',['$event']]]],['touchMove',[['ListTouchMove',['$event']]]],['touchEnd',[['ListTouchEnd',['$event']]]],['touchStart',[['ListTouchStart',['$event']]]]]}}" class="cover-view-marks" style="{{'height:'+(height)+';'+('width:'+(width)+';')}}" catchTap="__e" onTouchMove="__e" catchTouchEnd="__e" onTouchStart="__e"></view><view class="operating "><view class="dir-left-nowrap cross-bottom"><view class="left dir-left-nowrap main-between"><view class="up-down-goods"><view data-event-opts="{{[['tap',[['previous',['$event']]]]]}}" class="item" onTap="__e">上一件商品</view><view data-event-opts="{{[['tap',[['next',['$event']]]]]}}" class="item down" onTap="__e">下一件商品</view></view><view class="goods-list"><view hidden="{{!(goodBool)}}" class="scroll" style="{{'bottom:'+(getEmpty+157+'rpx')+';'}}"><view class="scroll-view"><view data-event-opts="{{[['tap',[['routeGo',['$0'],['goods_list.'+cur_index+'.page_url']]]]]}}" class="item-good dir-left-nowrap" onTap="__e"><view class="image-view cover-pic"><image class="cover-pic " src="{{goods_list[cur_index].cover_pic}}"></image><view>视频同款</view></view><view class="content dir-top-nowrap main-between"><view class="name t-omit-two">{{goods_list[cur_index].name}}</view><view class="pri-but dir-left-nowrap main-between cross-bottom"><view><view class="dir-left-nowrap"><block a:if="{{goods_list[cur_index].is_level==1&&goods_list[cur_index].is_negotiable!=1}}"><view class="member-price"><app-member-price vue-id="{{('3')+','+('1')}}" price="{{goods_list[cur_index].level_price}}" onVueInit="__l"></app-member-price></view></block><block a:if="{{goods_list[cur_index].vip_card_appoint.discount>0}}"><view class="app-sup-vip"><app-sup-vip vue-id="{{('4')+','+('1')}}" discount="{{goods_list[cur_index].vip_card_appoint.discount}}" is_vip_card_user="{{goods_list[cur_index].vip_card_appoint.is_vip_card_user}}" onVueInit="__l"></app-sup-vip></view></block></view><view class="pri"><text class="price">{{goods_list[cur_index].price_content}}</text><text class="original">{{"FCFA"+goods_list[cur_index].original_price}}</text></view></view><view data-event-opts="{{[['tap',[['specification',['$0'],['goods_list.'+cur_index+'']]]]]}}" class="button" catchTap="__e"></view></view></view></view></view><view class="triangle"></view></view><image class="image" src="/static/image/video-goods.png" data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" onTap="__e"></image></view></view><view class="right dir-left-nowrap"><view data-event-opts="{{[['tap',[['routeGo',['/pages/index/index']]]]]}}" class="but index" catchTap="__e"><view class="icon"><image class="image" src="/static/image/icon/video-index.png"></image></view><text>首页</text></view><view data-event-opts="{{[['tap',[['specification',['$0'],['goods_list.'+cur_index+'']]]]]}}" class="but cart" catchTap="__e"><view class="icon"><image class="image" src="/pages/goods/image/video-card.png"></image></view><text>加购</text></view><view class="but"><view class="icon"><image class="image" src="/static/image/icon/video-share.png"></image></view><text>分享</text><button class="share" open-type="share"></button></view></view></view><view class="empty" style="{{'height:'+(getEmpty+'rpx')+';'}}"></view></view><view class="attr"><app-attr vue-id="{{('5')+','+('1')}}" goods="{{item}}" select-attr="{{selectAttr}}" attr-group-list="{{attrGroup}}" show="{{show}}" cartShow="{{cartShow}}" buyText="{{buyText}}" buyClick="{{buyBool}}" plugin="{{plugin}}" previewUrl="{{previewUrl}}" submitUrl="{{submitUrl}}" ref="__r" data-ref="attr" data-event-opts="{{[['^buyClick',[['buyClick']]],['^attrtap',[['onAttr']]],['^attr',[['attr']]]]}}" onBuyClick="__e" onAttrtap="__e" onAttr="__e" onVueInit="__l"><block a:if="{{sign==='pintuan'}}"><view slot="extra"><block a:if="{{pt}}"><app-pt-attr vue-id="{{('6')+','+('5')}}" pintuan_groups="{{item.pintuan_groups}}" selectGroupAttrId="{{selectGroupAttrId}}" data-event-opts="{{[['^click',[['setGroupAttrID']]]]}}" onClick="__e" onVueInit="__l"></app-pt-attr></block></view></block></app-attr></view><block a:if="{{sign==='pintuan'&&show_pt}}"><view><app-iphone-x vue-id="{{('7')+','+('1')}}" onVueInit="__l"><view class="pintuan dir-left-nowrap" slot="empty-area"><block a:if="{{item}}"><view data-event-opts="{{[['tap',[['individual',['$event']]]]]}}" class="single box-grow-1 dir-top-nowrap" onTap="__e"><text class="app-text">单独购买</text></view></block><view data-event-opts="{{[['tap',[['multiplayer',['$event']]]]]}}" class="tuan box-grow-1 dir-top-nowrap" onTap="__e"><text class="app-text">拼团</text></view></view></app-iphone-x></view></block></view></app-layout>
