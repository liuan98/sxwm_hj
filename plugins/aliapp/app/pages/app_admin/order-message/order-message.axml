<app-layout vue-id="1" class="data-v-32d0233e" onVueInit="__l"><app-tab-nav vue-id="{{('2')+','+('1')}}" tabList="{{tabList}}" padding="0" activeItem="{{activeTab}}" theme="app-admin" data-event-opts="{{[['^click',[['tabStatus']]]]}}" onClick="__e" class="data-v-32d0233e" onVueInit="__l"></app-tab-nav><block a:for="{{list}}" a:for-item="item" a:for-index="__i0__" a:key="id"><view data-event-opts="{{[['tap',[['toOrder',['$0'],[[['list','id',item.id,'order_no']]]]]]]}}" onTap="__e" class="data-v-32d0233e"><view class="msg-item data-v-32d0233e"><block a:if="{{activeTab==8}}"><view class="msg-title data-v-32d0233e"><text class="plugin-name data-v-32d0233e">{{item.plugin_name}}</text>有新订单啦</view></block><block a:if="{{activeTab==4}}"><view class="msg-title data-v-32d0233e">退款申请</view></block><block a:if="{{activeTab==1&&item.type==2}}"><view class="msg-title cross-center data-v-32d0233e"><text class="replace data-v-32d0233e"></text>换货申请</view></block><block a:if="{{activeTab==1&&item.type==1}}"><view class="msg-title cross-center data-v-32d0233e"><text class="return data-v-32d0233e"></text>退货退款申请</view></block><view class="msg-time data-v-32d0233e">{{item.created_at}}</view><view class="goods-info data-v-32d0233e"><image class="goods-img data-v-32d0233e" src="{{activeTab==1?item.detail.goods_info.goods_attr.cover_pic:item.detail[0].goods.goodsWarehouse.cover_pic}}"></image><view class="goods-name t-omit-two data-v-32d0233e"><text class="data-v-32d0233e">{{activeTab==1?item.detail.goods_info.goods_attr.name:item.detail[0].goods.goodsWarehouse.name}}</text><block a:if="{{item.detail.length>1&&activeTab!=1}}"><text class="data-v-32d0233e">{{"等"+item.detail.length+"件商品"}}</text></block></view><view class="goods-user data-v-32d0233e">{{"来自用户"+item.nickname}}</view></view></view></view></block><block a:if="{{list.length==0}}"><view class="no-tip data-v-32d0233e"><image src="{{adminImg.no_message}}" class="data-v-32d0233e"></image><view class="data-v-32d0233e">没有任何消息哦~</view></view></block><block a:if="{{dialog}}"><view class="dialog data-v-32d0233e"><view class="picker-list data-v-32d0233e"><view class="main-between picker-header data-v-32d0233e"><view data-event-opts="{{[['tap',[['toggle',[false,2]]]]]}}" onTap="__e" class="data-v-32d0233e">取消</view><view data-event-opts="{{[['tap',[['toggle',[false,1]]]]]}}" onTap="__e" class="data-v-32d0233e">确定</view></view><picker-view indicator-style="height: 36px;" value="{{[index]}}" data-event-opts="{{[['change',[['bindChange',['$event']]]]]}}" onChange="__e" class="data-v-32d0233e"><picker-view-column class="data-v-32d0233e"><block a:for="{{choose_list}}" a:for-item="item" a:for-index="idx" a:key="name"><view class="{{((('data-v-32d0233e')+' '+'picker-view')+' '+[(newIndex===idx)?'sure-color':'',(newIndex===idx+1||newIndex===idx-1)?'cardinal-color':'',(newIndex===idx+2||newIndex===idx-2)?'even-color':''])}}">{{item.name+''}}</view></block></picker-view-column></picker-view></view></view></block></app-layout>