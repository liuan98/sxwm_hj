<app-layout vue-id="1" class="data-v-7cfe8b6a" onVueInit="__l"><view class="balance-detail-head cross-center data-v-7cfe8b6a"><view class="dir-left-nowrap cross-center data-v-7cfe8b6a"><view class="box-grow-0 key data-v-7cfe8b6a">交易金额</view><block a:if="{{detail.type==1}}"><view class="box-grow-1 value red data-v-7cfe8b6a">{{"+"+detail.money}}</view></block><block a:if="{{detail.type==2}}"><view class="box-grow-1 value blue data-v-7cfe8b6a">{{"-"+detail.money}}</view></block></view></view><view class="balance-detail data-v-7cfe8b6a"><view class="dir-left-nowrap info data-v-7cfe8b6a"><view class="box-grow-0 key data-v-7cfe8b6a">交易时间</view><view class="box-grow-1 value data-v-7cfe8b6a">{{detail.created_at}}</view></view><view class="dir-left-nowrap info data-v-7cfe8b6a"><view class="box-grow-0 key data-v-7cfe8b6a">交易详情</view><view class="box-grow-1 value data-v-7cfe8b6a">{{detail.desc}}</view></view><block a:if="{{detail.order_no}}"><view class="dir-left-nowrap info data-v-7cfe8b6a"><view class="box-grow-0 key data-v-7cfe8b6a">交易单号</view><view class="box-grow-1 value data-v-7cfe8b6a">{{detail.order_no}}</view></view></block><block a:if="{{detail.order_refund_no}}"><view class="dir-left-nowrap info data-v-7cfe8b6a"><view class="box-grow-0 key data-v-7cfe8b6a">退款单号</view><view class="box-grow-1 value data-v-7cfe8b6a">{{detail.order_refund_no}}</view></view></block></view></app-layout>