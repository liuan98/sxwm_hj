<app-layout vue-id="1" class="data-v-3edd3e62" onVueInit="__l"><block a:if="{{is_show}}"><view class="refund-box data-v-3edd3e62"><view class="goods-box data-v-3edd3e62"><app-jump-button vue-id="{{('2')+','+('1')}}" url="{{'/pages/goods/goods?id='+refundDetail.goods_id}}" class="data-v-3edd3e62" onVueInit="__l"><app-order-goods-info style="width:100%;" vue-id="{{('3')+','+('2')}}" goods="{{refundDetail.goods_info}}" class="data-v-3edd3e62" onVueInit="__l"></app-order-goods-info></app-jump-button></view><block a:if="{{switchBool}}"><view class="tab-box data-v-3edd3e62"><app-switch-tab vue-id="{{('4')+','+('1')}}" currentIndex="{{type}}" list="{{tabList}}" data-event-opts="{{[['^tabEvent',[['tabClick']]]]}}" onTabEvent="__e" class="data-v-3edd3e62" onVueInit="__l"></app-switch-tab></view></block><form class="form-box data-v-3edd3e62"><view class="dir-left-nowrap remark-box data-v-3edd3e62"><view class="box-grow-0 label data-v-3edd3e62">{{''+(type==0?'退款':'换货')+'原因'}}</view><view class="box-grow-1 data-v-3edd3e62"><app-textarea onInput="__e" vue-id="{{('5')+','+('1')}}" padding-y="0" padding-x="20" font-size="32" show-border="{{false}}" placeholder="{{'请输入'+(type==0?'退款':'换货')+'原因'}}" value="{{remark}}" data-event-opts="{{[['^input',[['__set_model',['','remark','$event',[]]]]]]}}" class="data-v-3edd3e62" onVueInit="__l"></app-textarea></view></view><block a:if="{{type==0}}"><block class="data-v-3edd3e62"><view class="dir-left-nowrap price-box data-v-3edd3e62"><view class="data-v-3edd3e62">退款金额</view><input class="price data-v-3edd3e62" name="refund_price" placeholder-style="color:#aaa" placeholder="{{'最多可退款'+refundDetail.refund_price}}" data-event-opts="{{[['input',[['__set_model',['','refund_price','$event',[]]]]]]}}" value="{{refund_price}}" onInput="__e"/></view></block></block><view class="image-box data-v-3edd3e62"><view class="label data-v-3edd3e62">上传凭证</view><app-upload-image vue-id="{{('6')+','+('1')}}" backgroundColor="#fff" maxNum="{{maxNum}}" data-event-opts="{{[['^imageEvent',[['imageEvent']]]]}}" onImageEvent="__e" class="data-v-3edd3e62" onVueInit="__l"></app-upload-image></view><view class="data-v-3edd3e62"><app-jump-button vue-id="{{('7')+','+('1')}}" class="data-v-3edd3e62" onVueInit="__l"><view data-event-opts="{{[['tap',[['formSubmit',['$event']]]]]}}" class="btn main-center cross-center data-v-3edd3e62" onTap="__e">提交</view></app-jump-button></view></form></view></block></app-layout>